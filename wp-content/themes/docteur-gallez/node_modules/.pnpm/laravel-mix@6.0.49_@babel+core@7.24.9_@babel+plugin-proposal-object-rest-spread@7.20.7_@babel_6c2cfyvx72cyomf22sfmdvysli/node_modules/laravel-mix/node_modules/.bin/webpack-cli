#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gdeketelaere/Ikonodule/Developments/wordpress/docteurgallez/app/public/wp-content/themes/docteur-gallez/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.93.0/node_modules/webpack-cli/bin/node_modules:/Users/gdeketelaere/Ikonodule/Developments/wordpress/docteurgallez/app/public/wp-content/themes/docteur-gallez/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.93.0/node_modules/webpack-cli/node_modules:/Users/gdeketelaere/Ikonodule/Developments/wordpress/docteurgallez/app/public/wp-content/themes/docteur-gallez/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.93.0/node_modules:/Users/gdeketelaere/Ikonodule/Developments/wordpress/docteurgallez/app/public/wp-content/themes/docteur-gallez/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gdeketelaere/Ikonodule/Developments/wordpress/docteurgallez/app/public/wp-content/themes/docteur-gallez/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.93.0/node_modules/webpack-cli/bin/node_modules:/Users/gdeketelaere/Ikonodule/Developments/wordpress/docteurgallez/app/public/wp-content/themes/docteur-gallez/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.93.0/node_modules/webpack-cli/node_modules:/Users/gdeketelaere/Ikonodule/Developments/wordpress/docteurgallez/app/public/wp-content/themes/docteur-gallez/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.93.0/node_modules:/Users/gdeketelaere/Ikonodule/Developments/wordpress/docteurgallez/app/public/wp-content/themes/docteur-gallez/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack-cli@4.10.0_webpack@5.93.0/node_modules/webpack-cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../webpack-cli@4.10.0_webpack@5.93.0/node_modules/webpack-cli/bin/cli.js" "$@"
fi
